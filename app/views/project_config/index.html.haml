- @title = "Project configuration page"

%h2 Project configuration

%div#projects
  = render("projects", :collection => @projects)

%hr
%h3 Add project configuration
- form_for(@project, :url => user_project_config_index_path(current_user), :remote => true) do |f|
  %p
    =f.label :key
    =f.text_field :key, :size => 100
  %p= f.submit "Search"

%div#search_results
