- if @configurations.count == 0
  %h3 (You have no configured projects)
- else
  - @config_by_key.keys.each do |key|
    %div.config_key{:id => "#{key}"}
      .title Api key: #{key}
      - configs = @config_by_key[key].sort_by(&:name)
      = render(:partial => "configuration", :collection => configs, :local => { :current_config => @current_config })
